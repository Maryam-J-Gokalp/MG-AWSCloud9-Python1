# BASH SCRIPT TEST BELOW ON VSC
bash-3.2$  # cat prime.sh
bash-3.2$ #!/bin/bash
> for ((i=1; i<=250; i++))
> do
> 
>       output=$(( $i % 2))
> 
>       if [ $output -ne 0 ]
> 
>       then
>       echo "We got odd numbers: $i"
>       fi
> done
